{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "migrate:dev": "npx dotenv -e .development.env -- npx prisma migrate dev",
    "migrate:prod": "npx dotenv -e .production.env -- npx prisma migrate dev",
    "migrate:stg": "npx dotenv -e .staging.env -- npx prisma migrate dev",
    "dev": "set NODE_ENV=development && nodemon index.js ",
    "prod": "set NODE_ENV=production&& nodemon index.js ",
    "stage": "set NODE_ENV=stage&& nodemon index.js ",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development --node-env=development",
    "build:stage": "webpack --mode=staging --node-env=staging",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "migrate:postgres": "dotenv -e .env.development -- npx prisma migrate deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "My webpack project",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "amqplib": "^0.10.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@webpack-cli/generators": "^3.0.7",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "workbox-webpack-plugin": "^7.1.0"
  }
}
